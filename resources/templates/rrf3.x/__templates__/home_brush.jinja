; home{{ letter }}.g
; called to home the A or B axis (brushes)
{% import '__macros__/move.jinja' as move %}

{% call move.rel_move() %}

M913 {{letter}}30                                           ; MOTORS TO 30% CURRENT
G1 H2 {{letter}}{{ -1 * (axis.width + 10) }} F50000
G1 H2 {{letter}}1                                           ; Back out a little bit
G92 {{letter}}{{ axis.min }}                                ; Mark {{ letter }} as homed

{% endcall %}

M913 {{letter}}100                   ; MOTORS TO 100% CURRENT
